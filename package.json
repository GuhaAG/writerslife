{
  "name": "writerslife",
  "version": "1.0.0",
  "private": false,
  "devDependencies": {
    "electron": "6.0.12",
    "foreman": "3.0.1",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "tailwindcss": "^1.1.2"
  },
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "start": "npm run tailwind:css && react-scripts start",
